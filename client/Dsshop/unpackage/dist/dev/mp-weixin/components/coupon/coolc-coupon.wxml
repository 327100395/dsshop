<view class="coupon-item"><view class="coupon-money"><block wx:if="{{item.seller_name}}"><view class="nick">{{item.seller_name+"使用"}}</view></block><block wx:if="{{item.type===3}}"><view class="layof" style="{{'color:'+(theme)+';'}}">{{item.money+"%"}}</view></block><block wx:else><view class="layof" style="{{'color:'+(theme)+';'}}">{{"￥"+item.money}}</view></block><block wx:if="{{item.ticket}}"><view class="tit">{{"券号："+item.ticket}}</view></block><block wx:if="{{item.title}}"><view class="demand">{{item.title}}</view></block><block wx:if="{{item.end_time}}"><view class="end_time">{{item.end_time+"前使用"}}</view></block><block wx:else><view class="time">{{"使用期限 "+item.time}}</view></block></view><block wx:if="{{item.state==='1'}}"><view><block wx:if="{{!types}}"><view data-event-opts="{{[['tap',[['getCoupon',['$0'],['item']]]]]}}" class="get-btn" style="{{'color:'+(color)+';'+('border-color:'+(color)+';')+('background:'+(solid)+';')}}" bindtap="__e">立即领取</view></block><block wx:else><navigator class="get-btn" style="{{'color:'+(color)+';'+('border-color:'+(color)+';')+('background:'+(solid)+';')}}" url="{{item.url}}" open-type="switchTab" hover-class="other-navigator-hover">立即使用</navigator></block></view></block><block wx:else><block wx:if="{{item.state==='2'}}"><view><block wx:if="{{!types}}"><view class="get-btn">已领取</view></block><block wx:else><view class="get-btn">已使用</view></block></view></block><block wx:else><view><block wx:if="{{!types}}"><view class="get-btn">已领取</view></block><block wx:else><view class="get-btn">已失效</view></block></view></block></block></view>